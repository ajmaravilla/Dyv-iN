
<div class="row">
	<div class="card mb-3 mt-3">
		<img class="card-img-top" src="/assets/images/Welcome3.png" alt="Card image cap">
		<div class="card-body">
			<h5 class="card-title">{{place.name}}</h5>
			<p>{{place.address}}</p>
		</div>
	</div>
	<div class="reviews">
		{{> establishmentReview}}
	</div>
</div>

<form id="posts">
  <div class="form-group">
    <label for="title">Title</label>
    <input placeholder="Post Title" type="text" class="form-control" id="title">
    <label for="body">Body</label>
    <textarea placeholder="Body" class="form-control" rows="10" id="postBody"></textarea>
    <button type="submit" class="btn btn-success submit">Submit</button>
  </div>
</form>

